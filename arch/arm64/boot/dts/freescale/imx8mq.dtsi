					<&clk IMX8MQ_CLK_ENET1_ROOT>,
					<&clk IMX8MQ_CLK_ENET_TIMER_DIV>,
					<&clk IMX8MQ_CLK_ENET_REF_DIV>,
					<&clk IMX8MQ_CLK_ENET_PHY_REF_DIV>,
					<&clk IMX8MQ_CLK_CLKO2_DIV>;
				clock-names = "ipg", "ahb", "ptp",
					"enet_clk_ref", "enet_out", "enet_phy_out";
